	<queryString>
		<![CDATA[SELECT
     rawat_jl_drpr.`no_rawat`,
     reg_periksa.`no_rkm_medis`,
     nm_pasien,
     nm_perawatan,
     nm_dokter,nama,
     biaya_rawat
FROM
     `pasien` pasien,
     `reg_periksa` reg_periksa,
     `jns_perawatan` jns_perawatan,petugas ,
     `dokter` dokter,
     `rawat_jl_drpr` rawat_jl_drpr
WHERE
     rawat_jl_drpr.no_rawat = reg_periksa.no_rawat
     and reg_periksa.no_rkm_medis = pasien.no_rkm_medis
     and rawat_jl_drpr.kd_jenis_prw = jns_perawatan.kd_jenis_prw
     and rawat_jl_drpr.kd_dokter = dokter.kd_dokter 
     and rawat_jl_drpr.nip=petugas.nip  
ORDER BY
     rawat_jl_drpr.no_rawat ASC]]>
	</queryString>
