  pegawai.id, 
                pegawai.nik, 
                pegawai.nama, 
                rekap_presensi.shift, 
                rekap_presensi.jam_datang, 
                rekap_presensi.jam_pulang, 
                rekap_presensi.status, 
                rekap_presensi.keterlambatan, 
                rekap_presensi.durasi, 
                rekap_presensi.keterangan 
