	<queryString>
		<![CDATA[SELECT
	reg_periksa.tgl_registrasi,
	reg_periksa.jam_reg,
	poliklinik.nm_poli,
	pasien.no_rkm_medis,
	pasien.nm_pasien,
	pasien.no_ktp,
	pasien.jk,
	pasien.tmp_lahir,
	pasien.tgl_lahir,
	pasien.nm_ibu,
	concat(
		pasien.alamat,
		', ',
		kelurahan.nm_kel,
		', ',
		kecamatan.nm_kec,
		', ',
		kabupaten.nm_kab
	) AS alamat,
	pasien.gol_darah,
	pasien.pekerjaan,
	pasien.stts_nikah,
	pasien.agama,
	pasien.tgl_daftar,
	pasien.no_tlp,
	pasien.umur,
	pasien.pnd,
	pasien.keluarga,
	pasien.namakeluarga,
	penjab.png_jawab,
	pasien.pekerjaanpj,
	concat(
		pasien.alamatpj,
		', ',
		pasien.kelurahanpj,
		', ',
		pasien.kecamatanpj,
		', ',
		pasien.kabupatenpj
	) AS alamatpj
FROM
	pasien
INNER JOIN kelurahan
INNER JOIN kecamatan
INNER JOIN kabupaten
INNER JOIN penjab ON pasien.kd_kel = kelurahan.kd_kel
AND pasien.kd_kec = kecamatan.kd_kec
AND pasien.kd_kab = kabupaten.kd_kab
INNER JOIN reg_periksa ON reg_periksa.no_rkm_medis = pasien.no_rkm_medis AND reg_periksa.kd_pj = penjab.kd_pj
INNER JOIN poliklinik ON poliklinik.kd_poli = reg_periksa.kd_poli]]>
	</queryString>
